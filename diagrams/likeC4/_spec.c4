specification {
  element softwareSystem {
    style {
      opacity 20%
    }
  }

  element enterprise {
    style {
      opacity 10%
      border solid
      color secondary
    }
  }

  element container {
    style {
      opacity 10%
      border dashed
      color green
    }
  }
  element component

